package tests.main;

import java.awt.Color;
import java.awt.Graphics;

import javax.swing.JFrame;

public class MovinSquare extends JFrame {
	
	private int x, y, velX, velY;
	
	public MovinSquare(Graphics g, Color color, int x, int y){
		this.x = x;
		this.y = y;
		g.setColor(color);
	}
	
	public int getX(){
		return x;
	}
	
	public int getY(){
		return y;
	}
	
	public void setX(int x){
		this.x = x;
	}

}
